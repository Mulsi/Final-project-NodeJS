<head>
    <% include partials/head %>
</head>
<div id="form">
    <p>Please input your new user</p>
    <hr />
    <form action="/signup" method="post">
        <div class="form-group">
            <label for="form_username">Username</label>
            <input id="form_username" type="text" name="username" />
        </div>
        <div class="form-group">
            <label for="form_password">Password</label>
            <input id="form_password" type="password" name="password" />
        </div>
        <div class="form-group">
            <label for="form_email">email</label>
            <input id="form_email" type="email" name="email" />
        </div>
        <button class="btn btn-primary btn-block" type="submit" value="Submit">Submit</button>
    </form>
    <hr />
</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.js"></script>
